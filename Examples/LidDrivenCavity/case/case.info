; Test case for the 2D lid driven cavity

CaseName LidDrivenCavity2D

Solver
{
	timeDependent On
	maxIterations 100
	timeStep 1
	maxTime 500
	maxCo 1e12
	numInnerIterations 1
	numPressureCorrections 2
	momentumRelaxation 0.9
	pressureCorrectionRelaxation 0.05
}

Properties
{
	rho 1
	mu 1e-2
	g (0,0)
	gamma 0.0001
}

Grid
{
	type rectilinear
	nCellsX 100
	nCellsY 100
	width 1
	height 1
}

Viewer
{
	type tecplot
	vectorFields u
	scalarFields p,pCorr,m
}

System
{
	fileWriteFrequency 15
}

; Initial conditions for the 2D rotating bubble

InitialConditions
{
  gamma
  {
    bubble
    {
      type circle
      radius 0.25
      center (0.5,0.5)
      value 1
    }
  }

  u
  {
    velocity
    {
      type rotating
      xFunction sin
      yFunction cos
      amplitude (-1,1)
      center (0.5,0.5)
    }
  }
}
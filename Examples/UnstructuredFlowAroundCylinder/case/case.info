; Test case for the 2D flow around unstructured cylinder

CaseName FlowAroundUnstructuredCylinder

Solver
{
	timeDependent Off
	maxIterations 100
	timeStep 0.00025
	maxTime 15
	maxCo 0.5

	numInnerIterations 1
	numPressureCorrections 2
	momentumRelaxation 1
	pressureCorrectionRelaxation 0.5
}

LinearAlgebra
{
	uEqn
	{
	  maxIterations 50
	  tolerance 1e-15
	  iluFill 1
	}

	pEqn
	{
	  maxIterations 300
	  tolerance 1e-15
	  iluFill 6
	}
}

Properties
{
	rho 1.81
	mu 1.81e-5
	g (0,0)
}

Grid
{
	type cgns
	filename "case/CylinderMesh.cgns"
}

Viewer
{
	type tecplot
	vectorFields u
	scalarFields p,m,rho,mu
}

Integrators
{
  ForceIntegrators
  {
    patches Cylinder
  }
}

System
{
  fileWriteFrequency 150
}

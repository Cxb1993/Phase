; Test case for the 2D stationary bubble

CaseName StationaryBubble2D

Solver
{
	timeDependent On
	maxIterations 100
	timeStep 2.5e-5
	maxCo 0.25
	maxTime 1

	momentumRelaxation 0.5
	pressureCorrectionRelaxation 0.5
	numInnerIterations 3
	numPressureCorrections 1

	smoothingKernelRadius 0.0000625
}

Properties
{
	rho1 998
	rho2 998
	mu1 8.94e-4
	mu2 8.94e-4
	sigma 0.0262
	g (0,0)
}

Grid
{
	type rectilinear
	nCellsI 200
	nCellsJ 200

	; Rectilinear parameters
	spacingX 0.000025
	spacingY 0.000025
}

Viewer
{
	type tecplot
	vectorFields u,n
	scalarFields p,pCorr,m,gamma,rho,mu,kappa,gammaTilde
}

System
{
  fileWriteFrequency 10
  consoleOutputFrequency 1
}

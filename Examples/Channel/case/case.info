; Test case for the 2D Channel

CaseName Channel2D

Solver
{
	timeDependent Off
	maxIterations 100
	timeStep 0.1
	maxTime 2.5
	maxCo 0.8

	numInnerIterations 3
	numPressureCorrections 2
	momentumRelaxation 0.5
	pressureCorrectionRelaxation 0.5
}

Properties
{
	rho 1.2
	mu 1e-2
	g (0,0)
	gamma 0.001
}

Grid
{
	type rectilinear
	nCellsX 80
	nCellsY 40

	; Rectilinear parameters
	width 0.8
	height 0.4
}

Viewer
{
	type tecplot
	vectorFields u,gradP
	scalarFields p,pCorr,m
}

System
{
  fileWriteFrequency 10
}
